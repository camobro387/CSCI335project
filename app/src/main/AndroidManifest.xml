<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.count.csci335project">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAnAccount"
            android:label="@string/create_acct_title"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".CreateAcctSuccessNotice"
            android:parentActivityName=".CreateAnAccount"/>
        <activity
            android:name=".DogDirectory"
            android:label="@string/title_activity_dog_directory" />
        <activity
            android:name=".Beyonce"
            android:label="Beyonce"
            android:parentActivityName=".DogDirectory" />
        <activity
            android:name=".JayZ"
            android:label="Jay-Z"
            android:parentActivityName=".DogDirectory" />
        <activity
            android:name=".Kim"
            android:label="Kim"
            android:parentActivityName=".DogDirectory" />
        <activity
            android:name=".Kanye"
            android:label="Kanye"
            android:parentActivityName=".DogDirectory" />
        <activity
            android:name=".Selena"
            android:label="Selena"
            android:parentActivityName=".DogDirectory" />
        <activity
            android:name=".JB"
            android:label="JB"
            android:parentActivityName=".DogDirectory" />
        <activity android:name=".OurMission" />
        <activity android:name=".ReservationActivity" />
        <activity
            android:name=".Favorites"
            android:label="My Favorites" />
        <activity
            android:name=".DonateActivity"
            android:parentActivityName=".OurMission" />
        <activity
            android:name=".DonateSuccessNotice"
            android:parentActivityName=".DonateActivity"></activity>
        <activity android:name=".MyReservations"
            android:label="My Reservations"/>
    </application>

</manifest>